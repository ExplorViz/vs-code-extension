{
  "name": "explorviz-vscode-extension",
  "displayName": "explorviz-vscode-extension",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:explorviz-vscode-extension.helloWorld",
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "explorviz-vscode-extension.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "explorviz-vscode-extension.webview",
        "title": "ExplorViz webview"
      },
      {
        "command": "explorviz-vscode-extension.testCommand",
        "title": "ExplorViz TestCommand Callback"
      },
      {
        "command": "explorviz-vscode-extension.VizDoubleClickOnMesh",
        "title": "ExplorViz VizDoubleClickOnMesh"
      },
      {
        "command": "explorviz-vscode-extension.VizSingleClickOnMesh",
        "title": "VizSingleClickOnMesh"
      },
      {
        "command": "explorviz-vscode-extension.IdeTestCallback",
        "title": "IdeTestCallback"
      }
    ],
    "menus": {
      "testing/item/gutter": [
        {
          "command": "explorviz-vscode-extension.helloWorld"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.16",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.18.11",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  },
  "dependencies": {
    "@types/socket.io": "^3.0.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "request-promise-native": "^1.0.9",
    "socket.io": "^4.5.4",
    "socket.io-client": "^4.5.4"
  }
}
